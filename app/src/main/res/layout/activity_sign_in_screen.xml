<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#FFFFFF"
	android:orientation="vertical">

	<ScrollView
		android:id="@+id/rposxqt2ffgn"
		android:layout_width="match_parent"
		android:layout_height="0dp"
		android:layout_weight="1"
		android:background="@drawable/signinscreen_3"
		android:elevation="10dp">

		<LinearLayout
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:orientation="vertical">

			<LinearLayout
				android:id="@+id/r0nrmap0g6eo"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_marginHorizontal="42dp"
				android:layout_marginTop="93dp"
				android:layout_marginBottom="80dp"
				android:gravity="center_horizontal"
				android:orientation="vertical"
				android:paddingVertical="8dp">

				<TextView
					android:id="@+id/remj37q5uhu"
					android:layout_width="94dp"
					android:layout_height="wrap_content"
					android:text="Hello \nSign In"
					android:textColor="#FFFFFF"
					android:textSize="30sp"
					android:textStyle="bold" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/rcya9tjge72a"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal"
				android:orientation="vertical">

				<LinearLayout
					android:id="@+id/r6qjmk1w89fx"
					android:layout_width="390dp"
					android:layout_height="wrap_content"
					android:background="@drawable/signinscreen_2"
					android:orientation="vertical"
					android:paddingTop="75dp"
					android:paddingBottom="60dp">

					<LinearLayout
						android:id="@+id/ruj9rdak8bjg"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginHorizontal="45dp"
						android:layout_marginBottom="1dp"
						android:orientation="vertical"
						android:paddingVertical="3dp">

						<TextView
							android:id="@+id/r2x6sc0fende"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginBottom="20dp"
							android:text="Email"
							android:textColor="#000000"
							android:textSize="16sp" />

						<TextView
							android:id="@+id/r7vzj3qrfe57"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="john@email.com"
							android:textColor="#888888"
							android:textSize="14sp" />
					</LinearLayout>

					<ImageView
						android:id="@+id/r28g5tlc6uz2"
						android:layout_width="match_parent"
						android:layout_height="1dp"
						android:layout_marginHorizontal="45dp"
						android:layout_marginBottom="53dp"
						android:scaleType="fitXY"
						android:src="@mipmap/ic_launcher" />

					<LinearLayout
						android:id="@+id/rznqe86qlgr"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginHorizontal="45dp"
						android:layout_marginBottom="41dp"
						android:orientation="vertical"
						android:paddingVertical="4dp">

						<TextView
							android:id="@+id/rmp9macpo8if"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginBottom="18dp"
							android:text="Password"
							android:textColor="#000000"
							android:textSize="16sp" />

						<RelativeLayout
							android:id="@+id/rlqg2lbeaxcf"
							android:layout_width="match_parent"
							android:layout_height="wrap_content">

							<LinearLayout
								android:layout_width="match_parent"
								android:layout_height="match_parent"
								android:orientation="vertical">

								<ImageView
									android:id="@+id/red7a1zf6hr"
									android:layout_width="47dp"
									android:layout_height="24dp"
									android:scaleType="fitXY"
									android:src="@mipmap/ic_launcher" />
							</LinearLayout>

							<ImageView
								android:id="@+id/rnz97qc1ddo9"
								android:layout_width="300dp"
								android:layout_height="1dp"
								android:layout_alignParentBottom="true"
								android:layout_marginStart="0dp"
								android:layout_marginBottom="3dp"
								android:scaleType="fitXY"
								android:src="@mipmap/ic_launcher" />
						</RelativeLayout>
					</LinearLayout>

					<LinearLayout
						android:id="@+id/rwvurafr9b6"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="46dp"
						android:gravity="end"
						android:orientation="vertical">

						<TextView
							android:id="@+id/r0ik133naawff"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginEnd="47dp"
							android:text="Forget password?"
							android:textColor="#000000"
							android:textSize="13sp" />
					</LinearLayout>

					<LinearLayout
						android:id="@+id/rfbx08q3kasl"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="104dp"
						android:gravity="center_horizontal"
						android:orientation="vertical">

						<LinearLayout
							android:id="@+id/ruscczjnjj2k"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:background="@drawable/signinscreen_1"
							android:clickable="true"
							android:focusable="true"
							android:gravity="center_horizontal"
							android:orientation="vertical"
							android:paddingVertical="17dp">

							<TextView
								android:id="@+id/r6w6uyvd5ldx"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Sign In"
								android:textColor="#FFFFFF"
								android:textSize="16sp"
								android:textStyle="bold" />
						</LinearLayout>
					</LinearLayout>

					<LinearLayout
						android:id="@+id/rgj7305v8j8"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:layout_marginBottom="10dp"
						android:gravity="end"
						android:orientation="vertical">

						<TextView
							android:id="@+id/rgdy9vgyg9qo"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginEnd="47dp"
							android:text="Dont have an account?"
							android:textColor="#888888"
							android:textSize="14sp" />
					</LinearLayout>

					<LinearLayout
						android:id="@+id/rsweqd1ehgf"
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						android:gravity="end"
						android:orientation="vertical">

						<TextView
							android:id="@+id/r19n3kpu4y4h"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginEnd="48dp"
							android:text="Sign Up"
							android:textColor="#000000"
							android:textSize="15sp"
							android:textStyle="bold" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</ScrollView>
</LinearLayout>
